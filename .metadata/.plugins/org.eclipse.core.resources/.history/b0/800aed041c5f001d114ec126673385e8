package leetcode.solutions;

public class Solution 
{
	public int removeDuplicates(int[] nums)
    { 
        int count = nums.length;
        int refNum = nums[0];
        for(int i=1; i<nums.length;i++)
        {
            if(refNum == nums[i])
            {
                nums[i] = 100;
                count--;
                // int refNum = nums[i-1];
            }
            else
            {
                refNum = nums[i];
            }
        }
        Arrays.sort(nums);
        return count;
    }
}
